{
	"tslint.alwaysShowRuleFailuresAsWarnings": true,
	"editor.formatOnSave": true,
	"editor.autoIndent": true,
	"editor.quickSuggestions": true,
	"editor.tabSize": 4,
	"material-icon-theme.folders.associations": {
		"module": "font",
		"component": "components",
		"doc": "markdown",
		"animation": "delta",
		"guard": "ngrx-effects",
		"directive": "ngrx-actions",
		"interceptor": "middleware",
		"validator": "coverage",
		"e2e": "test",
		"target": "dist",
		"rust": "delta",
		"benches": "benchmark"
	},
	"material-icon-theme.files.associations": {
		"*.animation.ts": "purescript",
		".mocharc": "mocha"
	},
	"typescript.tsdk": "node_modules\\typescript\\lib",
	"mochaExplorer.optsFile": "./mocha.opts",
	"mochaExplorer.files": "src/**/*.spec.ts",
	"mochaExplorer.mochaPath": "node_modules/mocha",
	"editor.codeActionsOnSave": {
		"source.fixAll": true
	},
	"editor.mouseWheelZoom": true,
	"files.associations": { ".mocharc": "json" },
	"theme-by-language.themes": {
		"*": "Material Theme Darker High Contrast",
		"typescript": "Material Theme Darker High Contrast",
		"rust": "1984 - Orwellian",
		"toml": "1984 - Orwellian"
	},
	"rust.build_on_save": true,
	"rust.clippy_preference": "on",
	"rust.racer_completion": true,
	"rust.show_hover_context": true
}
