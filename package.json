	"author": {
		"name": "Alex Aegis",
		"email": "alexaegis@gmail.com",
		"url": "https://github.com/AlexAegis"
	},
	"homepage": "https://github.com/AlexAegis",
	"bugs": {
		"email": "alexaegis@gmail.com",
		"url": "https://github.com/AlexAegis/advent-of-code/issues"
	},
	"scripts": {
		"lint": "tslint --project .",
		"lint:fix": "tslint --project . --fix",
		"format": "npx prettier --write \"src/**/*\"",
		"prettier": "npm run format",
		"test": "mocha --config ./.mocharc",
		"test:coverage": "nyc npm test",
		"codecov": "codecov",
		"check:updates": "ncu",
		"check:updates:update": "ncu -u"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/AlexAegis/advent-of-code/"
	},
	"categories": [
		"Other"
	],
	"license": "mit",
	"private": true,
	"dependencies": {
		"rxjs": "^6.5.2",
		"workerpool": "^4.0.0"
	},
	"devDependencies": {
		"@types/chai": "^4.2.0",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.7.2",
		"@types/workerpool": "^3.1.0",
		"chai": "^4.2.0",
		"codecov": "^3.5.0",
		"mocha": "^6.2.0",
		"nyc": "^14.1.1",
		"prettier": "^1.18.2",
		"source-map-support": "^0.5.13",
		"ts-node": "8.4.0",
		"tsconfig-paths": "^3.8.0",
		"tslint": "~5.19.0",
		"tslint-plugin-prettier": "^2.0.1",
		"npm-check-updates": "latest",
		"typescript": "3.5.3"
	},
	"nyc": {
		"extension": [
			".ts",
			".tsx"
		],
		"include": [
			"src/**/*.ts"
		],
		"exclude": [
			"node_modules/",
			"src/**/*.d.ts",
			"src/**/*.spec.ts",
			"src/**/deque.class.ts"
		],
		"reporter": [
			"lcov"
		],
		"all": true
	}
}
