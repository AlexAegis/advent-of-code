		"@types/is-ci": "^3.0.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "^16.9.6",
		"@types/workerpool": "^6.1.0",
		"@typescript-eslint/eslint-plugin": "^4.31.2",
		"@typescript-eslint/parser": "^4.31.2",
		"benny": "^3.6.15",
		"chai": "^4.3.4",
		"codecov": "^3.8.3",
		"commitlint": "^13.1.0",
		"concurrently": "^6.2.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"lint-staged": "^11.1.2",
		"live-server": "^1.2.1",
		"mocha": "^9.1.1",
		"npm-check-updates": "^11.8.5",
		"nyc": "^15.1.0",
		"prettier": "^2.4.1",
		"source-map-support": "^0.5.20",
		"ts-node": "10.2.1",
		"tsconfig-paths": "^3.11.0",
		"typescript": "4.4.3"
	},
	"engines": {
		"node": ">= 14.15.1",
		"npm": "7.x"
	},
	"nyc": {
		"extension": [
			".ts",
			".tsx"
		],
		"include": [
			"**/*.ts"
		],
		"exclude": [
			"node_modules/",
			"**/*.d.ts",
			"**/*.spec.ts",
			"**/deque.class.ts",
			"**/bench.ts"
		],
		"reporter": [
			"lcov"
		],
		"all": true
	}
}
