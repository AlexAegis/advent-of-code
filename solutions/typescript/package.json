		"ts-node": "9.1.1",
		"tsconfig-paths": "^3.9.0",
		"typescript": "4.1.3"
	},
	"engines": {
		"node": ">= 14.15.1",
		"npm": "7.x"
	},
	"nyc": {
		"extension": [
			".ts",
			".tsx"
		],
		"include": [
			"**/*.ts"
		],
		"exclude": [
			"node_modules/",
			"**/*.d.ts",
			"**/*.spec.ts",
			"**/deque.class.ts",
			"**/bench.ts"
		],
		"reporter": [
			"lcov"
		],
		"all": true
	}
