		"@types/mocha": "^8.2.0",
		"@types/node": "^14.14.20",
		"@types/workerpool": "^6.0.0",
		"@typescript-eslint/eslint-plugin": "^4.12.0",
		"@typescript-eslint/parser": "^4.12.0",
		"benny": "^3.6.15",
		"chai": "^4.2.0",
		"codecov": "^3.8.1",
		"commitlint": "^11.0.0",
		"concurrently": "^5.3.0",
		"eslint": "^7.17.0",
		"eslint-config-prettier": "^7.1.0",
		"eslint-plugin-prettier": "^3.3.1",
		"lint-staged": "^10.5.3",
		"live-server": "^1.2.1",
		"mocha": "^8.2.1",
		"npm-check-updates": "^10.2.5",
		"nyc": "^15.1.0",
