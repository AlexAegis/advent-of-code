{
	"$schema": "https://turborepo.org/schema.json?mark=managed-by-autotool",
	"globalDependencies": [
		".config/*",
		".env",
		".eslintrc.cjs",
		".prettierrc.cjs",
		".remarkrc.js",
		"package.json",
		"pnpm-lock.yaml",
		"tsconfig.json"
	],
	"globalPassThroughEnv": ["GITHUB_TOKEN"],
	"pipeline": {
		"//#all_": {
			"dependsOn": ["//#lint_", "//#typedoc_"],
			"outputMode": "new-only",
			"outputs": []
		},
		"//#format_": {
			"cache": false,
			"dependsOn": [],
			"inputs": [
				"!vite*.config.*.*",
				"!{.turbo,.cache,packages,libs,apps,fixtures,dist,build,.svelte-kit}/**/*",
				"**/*.{ts,js,tsx,jsx,md,json,json5,yaml,yml,html,svelte,vue,astro,xml,php}"
			],
			"outputMode": "new-only",
			"outputs": [".cache/prettier/**"]
		},
		"//#lint:depcheck_": {
			"dependsOn": [],
			"outputMode": "new-only",
			"outputs": []
		},
		"//#lint:es_": {
			"dependsOn": [],
			"outputMode": "new-only",
			"outputs": []
		},
		"//#lint:format_": {
			"dependsOn": [],
			"env": [],
			"inputs": [
				"!vite*.config.*.*",
				"!{.turbo,.cache,packages,libs,apps,fixtures,dist,build,.svelte-kit}/**/*",
				"**/*.{ts,js,tsx,jsx,md,json,json5,yaml,yml,html,svelte,vue,astro,xml,php}"
			],
			"outputMode": "new-only",
			"outputs": [".cache/prettier/**"]
		},
		"//#lint:md_": {
			"dependsOn": [],
			"inputs": [
				"!vite*.config.*.*",
				"!{packages,libs,apps,fixtures,.turbo,.cache,dist,build,.svelte-kit}",
				"**/*.md",
				"package.json"
			],
			"outputMode": "new-only",
			"outputs": []
		},
		"//#lint:svelte_": {
			"dependsOn": [],
			"outputMode": "new-only",
			"outputs": []
		},
		"//#lint:tsc_": {
			"dependsOn": [],
			"outputMode": "new-only",
			"outputs": [".cache/tsc/**"]
		},
		"//#lint_": {
			"dependsOn": [
				"//#lint:depcheck_",
				"//#lint:es_",
				"//#lint:format_",
				"//#lint:md_",
				"//#lint:tsc_"
			],
			"outputMode": "new-only",
			"outputs": []
		},
		"//#typedoc_": {
			"dependsOn": [],
			"inputs": [
				"!vite*.config.*.*",
				"!{.turbo,.cache,dist,build,.svelte-kit}/**/*",
				"typedoc.json",
				"{packages,libs,apps}/**/*.ts"
			],
			"outputMode": "new-only",
			"outputs": [".cache/typedoc/**"]
		},
		"all_": {
			"dependsOn": ["build-app_", "build-lib_", "lint_", "publint_", "test_", "typedoc_"],
			"outputMode": "new-only",
			"outputs": []
		},
		"build-app_": {
			"dependsOn": ["^build-lib_"],
			"env": ["BASE_HREF", "BUILD_REASON", "PLATFORM", "TARGET_ENV", "VERCEL"],
			"inputs": [
				"!vite*.config.*.*",
				"!{src,static,public}/**/*.spec.{ts,js,tsx,jsx}",
				"*.*",
				"{src,static,public}/**/*"
			],
			"outputMode": "new-only",
			"outputs": [".svelte-kit/**", "build/**", "dist/**", ".vercel/**"],
			"passThroughEnv": []
		},
		"build-lib_": {
			"dependsOn": ["^build-lib_"],
			"env": ["BASE_HREF", "BUILD_REASON", "PLATFORM", "TARGET_ENV"],
			"inputs": [
				"!vite*.config.*.*",
				"!{src,static,public}/**/*.spec.{ts,js,tsx,jsx}",
				"*.*",
				"{src,static,public}/**/*"
			],
			"outputMode": "new-only",
			"outputs": [".svelte-kit/**", "build/**", "dist/**"],
			"passThroughEnv": ["PLATFORM"]
		},
		"dev_": {
			"cache": false,
			"dependsOn": ["^build-lib_"],
			"passThroughEnv": ["BUILD_REASON"],
			"persistent": true
		},
		"format_": {
			"cache": false,
			"dependsOn": [],
			"inputs": [
				"!vite*.config.*.*",
				"!{.turbo,.cache,dist,build,.svelte-kit}/**/*",
				"*.*",
				"{src,static,public}/**/*.{ts,js,tsx,jsx,md,json,json5,yaml,yml,html,svelte,vue,astro,xml,php}"
			],
			"outputMode": "new-only",
			"outputs": [".cache/prettier/**"]
		},
		"lint:depcheck_": {
			"dependsOn": [],
			"outputMode": "new-only",
			"outputs": []
		},
		"lint:es_": {
			"dependsOn": [],
			"outputMode": "new-only"
		},
		"lint:format_": {
			"dependsOn": [],
			"env": [],
			"inputs": [
				"!vite*.config.*.*",
				"!{.turbo,.cache,dist,build,.svelte-kit}/**/*",
				"*.*",
				"{src,static,public}/**/*.{ts,js,tsx,jsx,md,json,json5,yaml,yml,html,svelte,vue,astro,xml,php}"
			],
			"outputMode": "new-only",
			"outputs": [".cache/prettier/**"]
		},
		"lint:md_": {
			"dependsOn": [],
			"inputs": ["!vite*.config.*.*", "!{.turbo,.cache}/**/*", "**/*.md", "package.json"],
			"outputMode": "new-only",
			"outputs": []
		},
		"lint:style:css_": {
			"dependsOn": [],
			"inputs": [".stylelint*", "{src,static,public}/**/*.css"],
			"outputMode": "new-only",
			"outputs": [".cache/stylelintcache-css"]
		},
		"lint:style:html_": {
			"dependsOn": [],
			"inputs": [".stylelint*", "{src,static,public}/**/*.{html,svelte,vue,astro,xml,php}"],
			"outputMode": "new-only",
			"outputs": [".cache/stylelintcache-html"]
		},
		"lint:style:scss_": {
			"dependsOn": [],
			"inputs": [".stylelint*", "{src,static,public}/**/*.{scss,sass}"],
			"outputMode": "new-only",
			"outputs": [".cache/stylelintcache-scss"]
		},
		"lint:style_": {
			"dependsOn": ["lint:style:css_", "lint:style:html_", "lint:style:scss_"],
			"outputMode": "new-only",
			"outputs": []
		},
		"lint:svelte_": {
			"dependsOn": ["^build-lib_"],
			"outputMode": "new-only",
			"outputs": []
		},
		"lint:tsc_": {
			"dependsOn": [],
			"outputMode": "new-only",
			"outputs": [".cache/tsc/**"]
		},
		"lint_": {
			"dependsOn": [
				"lint:depcheck_",
				"lint:es_",
				"lint:format_",
				"lint:md_",
				"lint:style:css_",
				"lint:style:html_",
				"lint:style:scss_",
				"lint:svelte_",
				"lint:tsc_"
			],
			"inputs": ["!**/*"],
			"outputMode": "new-only",
			"outputs": []
		},
		"publint_": {
			"dependsOn": ["build-lib_"],
			"env": ["BUILD_REASON"],
			"inputs": ["dist/**"],
			"outputMode": "new-only",
			"outputs": [],
			"passThroughEnv": ["BASE_HREF", "PLATFORM", "TARGET_ENV"]
		},
		"start_": {
			"cache": false,
			"dependsOn": ["^build-lib_"],
			"passThroughEnv": ["BUILD_REASON"],
			"persistent": true
		},
		"test_": {
			"dependsOn": ["^build-lib_"],
			"env": ["VITEST"],
			"inputs": ["!vite*.config.*.*", "*.*", "{src,static,public}/**/*"],
			"outputMode": "new-only",
			"outputs": ["coverage/**"],
			"passThroughEnv": ["BUILD_REASON"]
		},
		"typedoc_": {
			"dependsOn": [],
			"inputs": [
				"!vite*.config.*.*",
				"!{.turbo,.cache,dist,build,.svelte-kit}/**/*",
				"**/*.ts",
				"typedoc.json"
			],
			"outputMode": "new-only",
			"outputs": [".cache/typedoc/**"]
		}
	}
}
