{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"prebuild": {
			"outputs": ["dist/**"],
			"dependsOn": [],
			"outputMode": "new-only"
		},
		"//#prebuild:workspace": {
			"outputs": ["dist/**"],
			"dependsOn": [],
			"outputMode": "new-only"
		},
		"build": {
			"env": ["BASE_HREF"],
			"dependsOn": ["prebuild"],
			"outputs": ["dist/**"],
			"outputMode": "new-only"
		},
		"//#build:workspace": {
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint": {
			"outputs": [],
			"dependsOn": [
				"lint:es",
				"lint:svelte",
				"lint:style",
				"lint:md",
				"lint:rmd",
				"lint:format"
			],
			"outputMode": "new-only"
		},
		"//#lint:workspace": {
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint:es": {
			"outputs": [],
			"dependsOn": [],
			"outputMode": "new-only"
		},
		"//#lint:es:workspace": {
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint:svelte": {
			"outputs": [],
			"dependsOn": [],
			"outputMode": "new-only"
		},
		"//#lint:svelte:workspace": {
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint:style": {
			"outputs": [],
			"dependsOn": [],
			"outputMode": "new-only"
		},
		"//#lint:style:workspace": {
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint:md": {
			"outputs": [],
			"dependsOn": [],
			"outputMode": "new-only"
		},
		"//#lint:md:workspace": {
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint:rmd": {
			"env": [],
			"outputs": [],
			"dependsOn": [],
			"outputMode": "new-only"
		},
		"//#lint:rmd:workspace": {
			"env": [],
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint:format": {
			"env": [],
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"//#lint:format:workspace": {
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"typedoc": {
			"dependsOn": [],
			"outputs": ["typedoc/**"],
			"outputMode": "new-only"
		},
		"test": {
			"env": ["VITEST"],
			"dependsOn": ["^build"],
			"outputs": ["coverage"],
			"outputMode": "new-only"
		},
		"start": {
			"cache": false
		},
		"dev": {
			"cache": false
		}
	}
}
