{
	"$schema": "https://turborepo.org/schema.json?mark=autogenerated",
	"pipeline": {
		"prebuild_": {
			"outputs": ["dist/**"],
			"dependsOn": [],
			"outputMode": "new-only"
		},
		"//#prebuild_": {
			"outputs": ["dist/**"],
			"dependsOn": [],
			"outputMode": "new-only"
		},
		"build-lib_": {
			"dependsOn": ["^prebuild_", "^build-lib_"],
			"outputs": ["dist/**"],
			"outputMode": "new-only"
		},
		"build-app_": {
			"env": ["BASE_HREF"],
			"dependsOn": ["^prebuild_", "^build-lib_"],
			"outputs": ["dist/**"],
			"outputMode": "new-only"
		},
		"ce_": {
			"outputs": [],
			"dependsOn": ["lint_", "build-app_", "build-lib_", "test_", "typedoc_"],
			"outputMode": "new-only"
		},
		"lint_": {
			"outputs": [],
			"dependsOn": [
				"lint:es_",
				"lint:depcheck_",
				"lint:tsc_",
				"lint:svelte_",
				"lint:style:css_",
				"lint:md_",
				"lint:format_"
			],
			"outputMode": "new-only"
		},
		"lint:es_": {
			"dependsOn": [],
			"outputMode": "new-only"
		},
		"//#lint:es_": {
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint:depcheck_": {
			"outputs": [],
			"dependsOn": [],
			"outputMode": "new-only"
		},
		"//#lint:depcheck_": {
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint:tsc_": {
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"//#lint:tsc_": {
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint:svelte_": {
			"outputs": [],
			"dependsOn": [],
			"outputMode": "new-only"
		},
		"//#lint:svelte_": {
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint:style_": {
			"dependsOn": ["lint:style:css_", "lint:style:scss_"],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint:style:require:build-lib_": {
			"env": [],
			"dependsOn": ["build-lib_"],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint:style:css_": {
			"dependsOn": ["^lint:style:require:build-lib_", "lint:style:require:build-lib_"],
			"outputs": [".cache/stylelintcache-css"],
			"outputMode": "new-only"
		},
		"//#lint:style:css_": {
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint:style:scss_": {
			"dependsOn": ["^lint:style:require:build-lib_", "lint:style:require:build-lib_"],
			"outputs": [".cache/stylelintcache-scss"],
			"outputMode": "new-only"
		},
		"//#lint:style:scss_": {
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint:md_": {
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"//#lint:md_": {
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint:format_": {
			"env": [],
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"//#lint:format_": {
			"env": [],
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"typedoc_": {
			"dependsOn": [],
			"outputs": [".cache/typedoc/**"],
			"outputMode": "new-only"
		},
		"//#typedoc_": {
			"dependsOn": [],
			"outputs": [".cache/typedoc/**"],
			"outputMode": "new-only"
		},
		"test_": {
			"env": ["VITEST"],
			"dependsOn": ["^build-lib_", "^test:require:build-lib_", "test:require:build-lib_"],
			"outputs": ["coverage/**"],
			"outputMode": "new-only"
		},
		"test:require:build-lib_": {
			"env": [],
			"dependsOn": ["build-lib_"],
			"outputs": [],
			"outputMode": "new-only"
		},
		"start": {
			"cache": false
		},
		"dev": {
			"cache": false
		}
	}
}
